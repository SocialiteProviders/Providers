    "keywords": [
        "laravel",
        "oauth",
        "provider",
        "socialite",
        "telegram"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adnan RIHAN",
            "email": "adnan@rihan.fr"
        }
    ],
    "require": {
